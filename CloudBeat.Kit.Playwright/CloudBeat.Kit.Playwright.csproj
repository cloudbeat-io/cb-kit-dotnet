<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;</TargetFrameworks>
    <RootNamespace>$(MSBuildProjectName)</RootNamespace>
	<PackageProjectUrl>https://cloudbeat.io</PackageProjectUrl>
	<Company>CBNR Cloud Solutions Ltd.</Company>
	<Authors>CBNR Cloud Solutions Ltd.</Authors>
	<AssemblyVersion>4.1.1.0</AssemblyVersion>
	<FileVersion>4.1.1.0</FileVersion>
	<Version>4.1.1</Version>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/cloudbeat-io/cb-kit-dotnet</RepositoryUrl>
	<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
	<DebugType>embedded</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Playwright" Version="1.39.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\CloudBeat.Kit.Common\CloudBeat.Kit.Common.csproj" />
  </ItemGroup>
</Project>
